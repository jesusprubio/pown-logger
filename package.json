{
  "name": "pown-logger",
  "version": "0.1.2",
  "description": "Pownage guaranteed",
  "main": "index.js",
  "scripts": {
    "test": "tap test",
    "lint": "eslint .",
    "dep-ok": "depcheck --ignores=release-it",
    "dep-sec": "nsp check",
    "dep-old": "ncu",
    "posttest": "npm run lint && npm run dep-ok && npm run dep-sec",
    "release": "npm test && release-it",
    "release-minor": "npm test && release-it minor",
    "release-major": "npm test && release-it major",
    "logger": "POWN_ROOT=./ pown logger"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jesusprubio/pown-logger.git"
  },
  "keywords": [
    "exploit",
    "framework",
    "voip",
    "sip",
    "ami"
  ],
  "author": "Jesús Pérez <jesusprubio@fsf.org>",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesusprubio/ragnar/issues"
  },
  "homepage": "https://github.com/pownjs/pown#readme",
  "pown": {
    "commands": [
      "./index.js"
    ]
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "keypress": "^0.2.1",
    "lodash": "^4.17.4",
    "node-emoji": "^1.5.1",
    "prettyjson": "^1.2.1"
  },
  "devDependencies": {
    "depcheck": "^0.6.4",
    "eslint": "^3.6.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-json": "^1.2.0",
    "npm-check-updates": "^2.8.0",
    "nsp": "^2.1.0",
    "release-it": "^2.7.1",
    "tap": "^8.0.1"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  }
}
